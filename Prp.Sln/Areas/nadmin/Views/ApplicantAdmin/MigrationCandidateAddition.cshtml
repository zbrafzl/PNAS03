                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input' type='radio' id='semester" + (i + 1) + "StatusPass' name='semester" + (i + 1) + "Status' value='Pass'  onchange='handleSemesterChange(this)' /> <p> Pass</p></label>" +
                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input' type='radio' id='semester" + (i + 1) + "StatusFailed' name='semester" + (i + 1) + "Status' value='Failed' checked  onchange='handleSemesterChange(this)' /> <p> Failed</p></label>" +
                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input' type='radio' id='semester" + (i + 1) + "StatusSupply' name='semester" + (i + 1) + "Status' value='Supply'  onchange='handleSemesterChange(this)' /> <p> Supply</p></label></div>";
                    //"<select class='form-control' id='semester" + (i + 1) + "Status'  onchange='handleSemesterChange(this)'>" +
                    //"<option value='Pass'>Pass</option>" +
                    //"<option value='Failed' selected='selected'>Failed</option>" +
                    //"<option value='Supply'>Supply</option>" +
                //"</select>";
                semesterContainer.appendChild(semesterDiv);
            }

            for (var i = 1; i <= numberOfSemesters; i++) {
                document.getElementById("semester" + i).style.display = "none";
            }

            document.getElementById("semester1").style.display = "block";

        }

        function handleSemesterChange(selectElement) {
            debugger;

            inductionId = ConvertToInt($("#hfdInductionId").val());

            if (inductionId == 11) {
                numberOfSemesters = 2;
            } else if (inductionId == 10) {
                numberOfSemesters = 4;
            } else {
                numberOfSemesters = 6;
            }

            var semesterNumber = parseInt(selectElement.id.replace("semester", ""));
            var selectedStatus = selectElement.value;

            // If semester is passed, set the next semester's data to null and show the next semester's div
            if (selectedStatus === "Pass" && semesterNumber < numberOfSemesters) {
                for (var i = semesterNumber + 1; i < semesterNumber + 2; i++) {
                    var abc = document.getElementById("semester" + i + "StatusFailed");
                    document.getElementById("semester" + i + "StatusFailed").checked = true;
                    document.getElementById("semester" + i).style.display = "block";
                }
            }
            // If semester is failed or supply, hide subsequent divs and set their data to null
            else if (selectedStatus !== "Pass") {
                for (var i = semesterNumber + 1; i <= numberOfSemesters; i++) {
                    document.getElementById("semester" + i + "StatusFailed").checked = true;
                    document.getElementById("semester" + i).style.display = "none";
                }
            }
        }

        function saveData() {
            var semData = {};
            var listSemesterData = [];
            var numberOfSemesters = document.getElementsByClassName("semester").length;
            for (var i = 1; i <= numberOfSemesters; i++) {
                var status;
                if (document.getElementById("semester" + i + "StatusPass").checked == true){
                    status = "Pass";
                }
                else if (document.getElementById("semester" + i + "StatusFailed").checked == true) {
                    status = "Failed";
                }
                else if (document.getElementById("semester" + i + "StatusSupply").checked == true) {
                    status = "Supply";
                }
                listSemesterData.push({ semester: i, result: status });
                

                // If current semester is "Pass", show the next semester div
                //if (status === "Pass" && i < numberOfSemesters) {
                //    showSemesterDiv(i + 1);
                //}
            }
            semData.applicantId = applicantId;
            semData.semesterDatas = listSemesterData;
            var resp = CallActionPost("/ApplicantAdmin/UpdateMigrationSemesterData", semData);
            console.log(resp);
            // Call controller function to save data into SQL table
            // Example: Send data via AJAX to a controller method
        }

        function updateView(semesterResults) {
            debugger;
