                numberOfSemesters = 2;
            } else if (inductionId == 10) {
                numberOfSemesters = 4;
            } else {
                numberOfSemesters = 6;
            }


            var semesterContainer = document.getElementById("semesterContainer");
            for (var j = 0; j < semesterResults.length; j++) {
                var semesterDiv = document.createElement("div");
                semesterDiv.id = "semester" + semesterResults[j].semester;
                semesterDiv.className = "semester border border-info rounded text-center col-md-4";
                var abc = semesterResults[j].result;
                semesterDiv.innerHTML = "<div class='row'><span class='breadcrumb flex-lg-column' style='width:100%; text-align;center; font-weight: bold'>Semester " + (semesterResults[j].semester) + ": </h3></div><div class='w-100 p-3 col-md-12'>" +
                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input'  type='radio' id='semester" + semesterResults[j].semester + "StatusPass' name='semester" + semesterResults[j].semester + "Status' value='Pass' " + (semesterResults[j].result === "Pass" ? "checked='checked'" : "") + " onchange='handleSemesterChange(this)' /> <p> Pass</p></label>" +
                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input'  type='radio' id='semester" + semesterResults[j].semester + "StatusFailed' name='semester" + semesterResults[j].semester + "Status' value='Failed' " + (semesterResults[j].result === "Failed" ? "checked='checked'" : "") + " onchange='handleSemesterChange(this)' /> <p> Failed</p></label>" +
                    "<label class='col-md-4'><input style='margin-left:-15px; margin-top:25px;'  class='form-check-input'  type='radio' id='semester" + semesterResults[j].semester + "StatusSupply' name='semester" + semesterResults[j].semester + "Status' value='Supply' " + (semesterResults[j].result === "Supply" ? "checked='checked'" : "") + " onchange='handleSemesterChange(this)' /> <p> Supply</p></label></div>";
                semesterContainer.appendChild(semesterDiv);
            }

            // Hide higher semester divs if the current semester result is failed
            for (var i = 0; i < semesterResults.length; i++) {
                if (semesterResults[i].result === "Failed" || semesterResults[i].result === "Supply") {
                    hideHigherSemesters(i + 2);
                    break;
                }
            }
