        function GetApplicantMarksInfo() {
            //alert("GetApplicantInfo");
            if (1>0) {
                if (1>0) {
                    var resp = CallActionGet("/CommonAdmin/GetApplicantTentativeMarks?applicantId=" + applicantId
                        + "&fsc=100&gn=0&mw=0&bsn=0&extraQ=0&minExp=0&exp=0&entryTest=0");
                    console.log(resp);
                    $(".fsc").show();
                    $(".gn").hide();
                    $(".bsn").hide();
                    $(".postRn").hide();
                    $(".minExp").hide();
                    $(".exper").hide();
                    $(".entry").hide();
                    $(".total").show();
                    $("#fsc").html(resp.fscObtained);
                    $("#total").html(resp.fscObtained);
                    $(".obtainedAllQualification").html(resp.fscObtained);
                    $(".totalAllQualification").html("100");
                }
            }
            else if (@Convert.ToInt32(Session["degreeAchieved"]) == 12) {
                if (@Convert.ToInt32(Session["appliedFor"]) == 12) {
                    var resp = CallActionGet("/CommonAdmin/GetApplicantTentativeMarks?applicantId=" + applicantId
                        + "&fsc=0&gn=0&mw=0&bsn=90&extraQ=0&minExp=2&exp=10&entryTest=0");
                    console.log(resp);
                    $(".fsc").hide();
                    $(".gn").hide();
                    $(".bsn").show();
                    $(".postRn").hide();
                    $(".minExp").show();
                    $(".exper").show();
                    $(".entry").hide();
                    $(".total").show();
                    $("#minExp").html("2 Years Post Qualification");
                    $("#bsn").html(resp.bsnObtained);
                    $("#exper").html(resp.expObtained);
                    $("#total").html(parseFloat(resp.bsnObtained + resp.expObtained).toFixed(2));
                    //$("#total").html(resp.bsnObtained + resp.expObtained);
                    $(".obtainedQualification").html(resp.bsnObtained);
                    $(".totalQualification").html("90");
                    $(".obtainedAllQualification").html(resp.bsnObtained);
                    $(".totalAllQualification").html("90");
                    $(".obtainedExperience").html(resp.expObtained);
                    $(".totalExperience").html("10");
                }
            }
            else if (@Convert.ToInt32(Session["degreeAchieved"]) == 11) {
                //alert('here');
                if (@Convert.ToInt32(Session["appliedFor"]) == 11) {
                    //alert('jere');
                    var resp = CallActionGet("/CommonAdmin/GetApplicantTentativeMarks?applicantId=" + applicantId
                        + "&fsc=0&gn=25&mw=25&bsn=0&extraQ=2&minExp=2&exp=8&entryTest=40");
                    console.log(resp);
                    $(".fsc").hide();
                    $(".gn").show();
                    $(".bsn").hide();
                    $(".postRn").show();
                    $(".minExp").show();
                    $(".exper").show();
                    $(".entry").show();
                    $(".total").show();
                    $("#minExp").html("2 Years Post Qualification");
                    $("#gn").html(parseFloat(resp.gnObtained + resp.mwObtained).toFixed(2));
                    $("#postRn").html(resp.extraObtained);
                    $("#exper").html(resp.expObtained);
                    $("#entry").html("0");
                    $("#entryTotal").html("40");
